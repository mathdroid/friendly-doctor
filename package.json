{
  "name": "vrnext",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:dev": "node -e \"console.log('open browser at http:/localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "test": "jest",
    "build": "npm run bundle",
    "start": "node serve"
  },
  "dependencies": {
    "ovr-audio": "~0.1.0",
    "ovr-video": "~0.1.0",
    "ovrui": "~0.1.0",
    "react": "~15.4.1",
    "react-native": "~0.42.0",
    "react-vr": "~0.2.0",
    "react-vr-web": "~0.2.0",
    "serve": "^5.1.2",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "^19.0.2",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  },
  "engines": {
    "node": "7.x.x"
  }
}
